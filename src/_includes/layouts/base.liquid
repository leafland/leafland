              {% endif %}

            {% elsif tree.form.size > 0 %}

            "name": "{{ tree.genus }} {{ tree.species }} f. {{ tree.form }} | {{ tree.commonName }}",

            {% elsif tree.variety.size > 0 %}

            "name": "{{ tree.genus }} {{ tree.species }} var. {{ tree.variety }}  | {{ tree.commonName }}",

