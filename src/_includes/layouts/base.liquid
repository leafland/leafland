      {% include "components/css/order.min.css" %}
      {% include "components/css/search.min.css" %}
      {% include "components/css/page.min.css" %}

    {% elsif type == 'top-trees' %}
      {% include "components/css/main.min.css" %}
      {% include "components/css/login.min.css" %}
      {% include "components/css/menu.min.css" %}
      {% include "components/css/order.min.css" %}
      {% include "components/css/search.min.css" %}
      {% include "components/css/top-trees.min.css" %}

    {% elsif type == 'retail' %}
      {% include "components/css/main.min.css" %}
      {% include "components/css/login.min.css" %}
      {% include "components/css/menu.min.css" %}
      {% include "components/css/order.min.css" %}
      {% include "components/css/search.min.css" %}
      {% include "components/css/stock-list.min.css" %}

    {% elsif type == 'wholesale' %}
      {% include "components/css/main.min.css" %}
      {% include "components/css/login.min.css" %}
      {% include "components/css/menu.min.css" %}
      {% include "components/css/order.min.css" %}
      {% include "components/css/search.min.css" %}
      {% include "components/css/stock-list.min.css" %}

    {% endif %}
    </style>

    {% if type == "product" %}

      {% include "components/head/_product.liquid"%}

    {% elsif type == "submit-order" %}
      {% include "components/head/_submit-order.liquid"%}

    {% elsif type == "tree-finder" %}
      {% include "components/head/_tree-finder.liquid"%}

    {% elsif type == 'index' %}
      {% include "components/head/_index.liquid"%}

    {% elsif type == 'page' %}
      {% include "components/head/_page.liquid"%}

    {% elsif type == 'top-trees' %}
      {% include "components/head/_top-trees.liquid"%}

    {% elsif type == 'retail' %}
      {% include "components/head/_retail-list.liquid"%}

    {% elsif type == 'wholesale' %}
      {% include "components/head/_wholesale-list.liquid"%}

    {% endif %}

    <link rel="shortcut icon" type="image/svg+xml" href="https://leafland.co.nz/public/assets/logo/leafland-favicon.svg">
    <link rel="alternate icon" type="image/png" href="https://leafland.co.nz/public/assets/logo/leafland-favicon.png">

    {% if type == 'product' %}

        <script type="application/ld+json">
          {
            "@context": "https://schema.org/", 
            "@type": "Product", 
            "name": "{% if commonName == "" %}{{ botanicalName | escape}}{% else %}{{ botanicalName | escape}} ({{ commonName | escape}}){% endif %}",
            "botanicalName": "{{ botanicalName | escape}}",
            {% unless commonName == "" %}"commonName": "{{ commonName | escape}}",{% endunless %}
            "image": "https://images.leafland.co.nz/images/trees/{{ mainImage }}?tr=w-1500,q-75,pr-true,f-auto",
            "description": "{{ description | escape}}",
            "url":"https://leafland.co.nz/trees/{{ url }}/",
            "height": "{{ height }}",
            "width": "{{ width }}",
            "winterFoliage": "{{ winterFoliage | capitalize }}",
            "origin": "{{ origin | capitalize }}",
            "uses": "{{ uses | replace: '-', ' ' | capitalize }}",
            "types": "{{ types | replace: '-', ' ' | capitalize }}",
            "soilType": "{{ soilType | replace: '-', ' ' | capitalize }}",
            "tolerates": "{{ tolerates | replace: '-', ' ' | capitalize }}",
            "sunShade": "{{ sunShade | replace: '-', ' ' | capitalize }}",
            "flowerColour": "{{ flowerColor | capitalize }}",
            "autumnColour": "{{ autumnColor | capitalize }}",
            "foliageColour": "{{ foliageColor | capitalize }}",
            "fruitingSeason": "{{ fruitingSeason | capitalize }}",
            "floweringSeason": "{{ floweringSeason | capitalize }}",
            "aggregateRating": {
              "@type": "AggregateRating",
              "ratingValue": "5",
              "reviewCount": "10"
            },
            "brand": {
              "@type": "Organization",
              "name": "Leafland"
            },
            "publisher": {
              "@type": "Organization",
              "name": "Leafland Limited",
              "logo": {
                "@type": "ImageObject",
                "url": "https://leafland.co.nz/public/assets/logo/leafland-logo.svg"
              }
            }
          }
        </script>

    {% else %}

      <script type="application/ld+json">
        {
            "@context": "http://schema.org",
            "@type": "WebPage",
            "name": "{{ title }}",
            "description": "{{description }}",
            "publisher": {
                "@type": "Organization",
                "name": "Leafland Limited",
                "logo": {
                    "@type": "ImageObject",
                    "url": "https://leafland.co.nz/public/assets/logo/leafland-logo.svg"
                }
            }
        }
      </script>
    {% endif %}

    <script defer data-domain="leafland.co.nz" src="https://plausible.io/js/plausible.js"></script>
    <script defer src="https://www.googletagmanager.com/gtag/js?id=UA-207729468-1"></script>
    <script defer src="/public/assets/analytics.min.js"></script>

  </head>

  <body>

    {% include "components/_header.liquid"%}

    <main>
      {{ content }}

      {% include "components/_footer.liquid"%}
    </main>

    {% include "components/_search.liquid"%}
    
    {% include "components/_login.liquid"%}

    {% include "components/_order.liquid"%}

    {% include "components/_main-menu.liquid"%}

    {% if type == 'product' %}

    {% include "components/_image-lightbox.liquid"%}

    {% endif %}

  </body>

</html>
