                {% elsif link contains "havlis.cz" %}

                <p><a href="{{ link }}" target="_blank" rel="noopener noreferrer">Milan Havlis - Specialist Garden Centre</a><p>

                {% elsif link contains "temperate.theferns.info" %}

                <p><a href="{{ link }}" target="_blank" rel="noopener noreferrer">Useful Temperate Plants</a><p>

                {% elsif link contains "tropical.theferns.info" %}

                <p><a href="{{ link }}" target="_blank" rel="noopener noreferrer">Useful Tropical Plants</a><p>

                {% elsif link contains "ebben.nl" %}

                <p><a href="{{ link }}" target="_blank" rel="noopener noreferrer">Ebben Nurseries</a><p>

                {% elsif link contains "gardenia.net" %}

                <p><a href="{{ link }}" target="_blank" rel="noopener noreferrer">Gardenia</a><p>

                {% elsif link contains "camellia.iflora.cn" %}

                <p><a href="{{ link }}" target="_blank" rel="noopener noreferrer">International Camellia Register</a><p>

                {% elsif link contains "citscihub.nz" %}

                <p><a href="{{ link }}" target="_blank" rel="noopener noreferrer">Citizen Science Hub</a><p>

                {% else %}

                <p><a href="{{ link }}" target="_blank" rel="noopener noreferrer">{{ link }}</a><p>

                {% endif %}

            {% endfor %}

            </div>
            
        {% endif %}
    
    </div>

</div>