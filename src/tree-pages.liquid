---
pagination:
    addAllPagesToCollections: true
    data: trees
    size: 1
    alias: tree
permalink: "trees/{{ tree.url }}/"
layout: layouts/product.liquid
type: product
eleventyComputed:
    code: "{{ tree.code }}"
    synonyms: {{ tree.synonyms }}
    commonName: "{{ tree.commonName }}"
    otherCommonNames: "{{ tree.otherCommonNames }}"
    fullName: "{{ tree.fullName }}"
    url: "{{ tree.url }}"
    description: "{{ tree.description }}"
    uses: "{{ tree.uses }}"
    tolerates: "{{ tree.tolerates }}"
    soilType: "{{ tree.soilType }}"
    sunShade: "{{ tree.sunShade }}"
    winterFoliage: "{{ tree.winterFoliage }}"
    origin: "{{ tree.origin }}"
    types: "{{ tree.types }}"
    reproduction: "{{ tree.reproduction }}"
    height: "{{ tree.height }}"
    width: "{{ tree.width }}"
    stem: {{ tree.stem }}
    flowers: {{ tree.flowers }}
    foliage: {{ tree.foliage }}
    fruit: {{ tree.fruit }}
    treeShape: "{{ tree.treeShape }}"
    images: {{ tree.images }}
---