---
pagination:
    addAllPagesToCollections: true
    data: trees
    size: 1
    alias: tree
permalink: "trees/{{ tree.url }}/"
layout: layouts/product.liquid
type: product
eleventyComputed:
    family: "{{ tree.family }}"
    genus: "{{ tree.genus }}"
    species: "{{ tree.species }}"
    hybrid: "{{ tree.hybrid }}"
    subspecies: "{{ tree.subspecies }}"
    variety: "{{ tree.variety }}"
    form: "{{ tree.form }}"
    cultivar: "{{ tree.cultivar }}"
    synonyms: {{ tree.synonyms }}
    commonName: "{{ tree.commonName }}"
    otherCommonNames: "{{ tree.otherCommonNames }}"
    url: "{{ tree.url }}"
    mainImage: "{{ tree.mainImage }}"
    description: "{{ tree.description }}"
    uses: "{{ tree.uses }}"
    tolerates: "{{ tree.tolerates }}"
    soilType: "{{ tree.soilType }}"
    sunShade: "{{ tree.sunShade }}"
    winterFoliage: "{{ tree.winterFoliage }}"
    origin: "{{ tree.origin }}"
    types: "{{ tree.types }}"
    height: "{{ tree.height }}"
    width: "{{ tree.width }}"
    stem: {{ tree.stem }}
    flowers: {{ tree.flowers }}
    foliage: {{ tree.foliage }}
    fruit: {{ tree.fruit }}
    treeShape: "{{ tree.treeShape }}"
    relatedTrees: {{ tree.relatedTrees }}
    images: {{ tree.images }}
    links: {{ tree.links }}
---