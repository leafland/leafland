---
pagination:
    addAllPagesToCollections: true
    data: trees
    size: 1
    alias: tree
permalink: "trees/{{ tree.url }}/"
layout: layouts/product.liquid
type: product
eleventyComputed:
    botanicalName: "{{ tree.botanicalName }}"
    commonName: "{{ tree.commonName }}"
    description: "{{ tree.description }}"
    mainImage: "{{ tree.mainImage }}"
    images: "{{ tree.images }}"
    url: "{{tree.url }}"
    treeShape: "{{tree.treeShape}}"
    stem: "{{tree.stem}}"
    foliage: "{{tree.foliage}}"
    flowers: "{{tree.flowers}}"
    fruit: "{{tree.fruit}}"
    tolerates: "{{tree.tolerates}}"
    soilType: "{{tree.soilType}}"
    sunShade: "{{tree.sunShade}}"
    uses: "{{tree.uses}}"
    types: "{{tree.types}}"
    winterFoliage: "{{tree.winterFoliage}}"
    origin: "{{tree.origin}}"
    height: "{{tree.height}}"
    width: "{{tree.width}}"
---